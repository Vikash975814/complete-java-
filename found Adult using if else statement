import java.util.*;

public class Base {
    public static void main(String s[]) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the age");
        int age = sc.nextInt();
        if (age >= 18) {
            System.out.println("adult");
        } else {
            System.out.println("not adult");
        }
    }
}
