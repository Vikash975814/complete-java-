import java.util.*;

public class Base {
    public static void main(String s[]) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number");
        int num = sc.nextInt();
        int i = 0;
        while (i < num) {
            System.out.println("* * * *");
            i++;
        }
    }
}
