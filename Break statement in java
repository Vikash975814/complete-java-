import java.util.*;

public class Base {
    public static void main(String s[]) {
        Scanner sc = new Scanner(System.in);
        do {
            System.out.println("enter the number");
            int num = sc.nextInt();
            if (num % 10 == 0) {
                break;
            }
            System.out.println(num);
        } while (true);

    }
}
