import java.util.*;

public class Base {
    public static void main(String s[]) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number");
        int n = sc.nextInt();
        if (n == 2) {
            System.out.println("Number is prime");
        } else {
            boolean isprime = true;
            for (int i = 2; i < n; i++) {
                if (n % i == 0) {
                    isprime = false;
                }
            }
            if (isprime == true) {
                System.out.println("Number is prime");
            } else {
                System.out.println("Number is not prime");
            }
        }
    }
}
