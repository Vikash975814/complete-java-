public class JavaPromotion {
    public static void main(String s[]) {
        byte b = 4;
        char c = 'a';
        short S = 512;
        int $ = 1000;
        float f = 3.14f;
        double d = 99.9954;
        double result = (f * b) + ($ % c) - (d * S);
        System.out.println(result);
    }
}
